{
  "name": "puppeteer-lazy-screenshots",
  "version": "1.0.0",
  "description": "A puppeteer script that takes full screenshots and awaits lazy loaded content",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:watch": "npx nodemon src/index.ts --",
    "test": "NODE_ENV=TEST jest --runInBand --detectOpenHandles"
  },
  "keywords": [
    "puppeteer",
    "typescript",
    "nodejs",
    "node",
    "puppeteer-autoscroll",
    "screenshots",
    "full-page"
  ],
  "author": "RustyDev",
  "license": "ISC",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "dotenv": "^16.0.3",
    "puppeteer": "^19.8.0",
    "puppeteer-autoscroll-down": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "minimist": "^1.2.8",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
